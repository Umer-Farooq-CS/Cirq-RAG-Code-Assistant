{
  "app": {
    "name": "Cirq-RAG-Code-Assistant",
    "version": "0.1.0",
    "debug": true,
    "environment": "development"
  },
  "api_keys": {
    "openai_api_key": null,
    "huggingface_api_key": null,
    "anthropic_api_key": null
  },
  "models": {
    "embedding": {
      "model_name": "BAAI/bge-base-en-v1.5",
      "embedding_dimension": 768,
      "device": "auto",
      "batch_size": 16,
      "cache_folder": ".cache/embeddings"
    },
    "llm": {
      "provider": "ollama",
      "model": "qwen2.5-coder:14b-instruct-q4_K_M",
      "temperature": 0.2,
      "max_tokens": 1000
    },
    "fine_tuning": {
      "use_fine_tuned_model": false,
      "fine_tuned_model_path": null,
      "fine_tuning_dataset_path": null
    }
  },
  "rag": {
    "knowledge_base": {
      "path": "data/knowledge_base",
      "size_target": 2500
    },
    "vector_store": {
      "type": "faiss",
      "index_path": "data/models/vector_index",
      "use_gpu": false
    },
    "retrieval": {
      "top_k_results": 3,
      "similarity_threshold": 0.6,
      "chunk_size": 512,
      "chunk_overlap": 50
    }
  },
  "agents": {
    "general": {
      "max_retries": 2,
      "timeout_seconds": 180,
      "parallel_execution": false
    },
    "designer": {
      "enabled": true,
      "use_rag": true
    },
    "optimizer": {
      "enabled": true,
      "level": "conservative",
      "use_rl": false,
      "rl_reward_weights": {
        "circuit_depth": -0.3,
        "two_qubit_gates": -0.3,
        "fidelity": 0.4
      },
      "rl_iterations": 5
    },
    "validator": {
      "enabled": true,
      "validation_level": "basic"
    },
    "educational": {
      "enabled": true,
      "explanation_depth": "beginner"
    }
  },
  "tools": {
    "compiler": {
      "validate_syntax": true,
      "check_imports": true
    },
    "simulator": {
      "type": "simulator",
      "repetitions": 100,
      "use_noise": false
    },
    "analyzer": {
      "calculate_connectivity": true,
      "detect_optimizations": true
    }
  },
  "paths": {
    "data_dir": "data",
    "datasets_dir": "data/datasets",
    "knowledge_base_dir": "data/knowledge_base",
    "models_dir": "data/models",
    "outputs_dir": "outputs",
    "logs_dir": "outputs/logs",
    "reports_dir": "outputs/reports",
    "artifacts_dir": "outputs/artifacts",
    "cache_dir": ".cache",
    "embedding_cache_dir": ".cache/embeddings"
  },
  "pytorch": {
    "cuda_visible_devices": "0",
    "torch_device": "auto",
    "torch_deterministic": false,
    "torch_benchmark": true,
    "torch_memory_fraction": 0.8
  },
  "evaluation": {
    "target_syntax_accuracy": 0.90,
    "evaluation_batch_size": 16,
    "benchmark_dataset_path": "data/datasets/benchmark",
    "benchmark_algorithms": ["vqe", "qaoa", "grover"],
    "enable_ablations": false,
    "ablation_variants": []
  },
  "logging": {
    "log_level": "DEBUG",
    "log_file": "outputs/logs/cirq_rag_dev.log",
    "log_format": "text",
    "enable_console_logging": true,
    "enable_file_logging": true,
    "max_log_file_size": "10MB",
    "log_backup_count": 3
  }
}

